{
    "version": 2,
    "builds": [
        {
            "src": "./index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "./index.ts"
        }
    ],
    "crons": [
        {
        "path": "/cron.ts",
        "schedule": "0 */10 * * * *"
      }
    ]
}
